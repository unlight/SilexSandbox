{
    "require": {
        "silex/silex": "1.0.*",
        "JosephLenton/PHP-Error": "*",
        "minmb/phpmailer": "dev-master",
        "gabordemooij/redbean": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "soup/paginator": "dev-master",
        "mikecao/sparrow": "*",
        "hybridauth/hybridauth": "dev-master",
        "hautelook/phpass": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "JosephLenton/PHP-Error",
                "version": "1.1.2",
                "source": {
                    "url": "https://github.com/JosephLenton/PHP-Error.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": {
                        "php_error\\ErrorHandler": "src/php_error.php"
                    }
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "mikecao/sparrow",
                "version": "1.2011",
                "source": {
                    "url": "https://github.com/mikecao/sparrow.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["sparrow.php"]
                }
            }
        }
    ]
}