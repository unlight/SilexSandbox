{
    "require": {
        "silex/silex": "1.0.*",
        "JosephLenton/PHP-Error": "*",
        "minmb/phpmailer": "dev-master",
        "gabordemooij/redbean": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "soup/paginator": "dev-master",
        "mikecao/sparrow": "*",
        "hybridauth/hybridauth": "dev-master",
        "opauth/twitter": "dev-master",
        "opauth/facebook": "dev-master",
        "opauth/google": "dev-master",
        "hautelook/phpass": "dev-master",
        "erivello/facebook-service-provider": "dev-master",
        "session-handler-cookie/session-handler-cookie": "dev-master",
        "iamthes/useful-functions": "master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "fabpot/goutte": "1.0.*@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "classmap": [
            "services", "controllers"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "JosephLenton/PHP-Error",
                "version": "master",
                "source": {
                    "url": "https://github.com/JosephLenton/PHP-Error.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": [
                        "src/php_error.php"
                    ]
                }
            }
        }, {
            "type": "package",
            "package": {
                "name": "mikecao/sparrow",
                "version": "master",
                "source": {
                    "url": "https://github.com/mikecao/sparrow.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["sparrow.php"]
                }
            }
        }
    ]
}